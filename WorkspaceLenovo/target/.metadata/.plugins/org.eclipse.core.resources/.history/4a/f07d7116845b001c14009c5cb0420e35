#include <stdint.h>



int main(void)
{
    uint32_t *clkreg=(uint32_t*) 	0x40021000+0x18;
    uint32_t *modereg=(uint32_t*)	0x40011000+0x04;
    uint32_t *outreg=(uint32_t*) 	0x40011000+0x0C;

    //Enable the clock for GPIOC peripheral in the APB2 bus
    *clkreg |=0x00000010;
    //clear bit
    *modereg $=0xff0fffff;
    //set bit
    *modereg |=0x00200000;
    //make 13th bit as High to enable LED
    *outreg |=0x00002000


}
