angular.forEach(e,function(a,b){"searchHistory"==b&&(a=angular.fromJson(a),c.items=angular.fromJson(a).sort(compare),c.$apply())})};c.searchHistoryResults=function(a){closePopoverSearch();var b=a.indexOf("q")+2,c=a.indexOf("\x26");b=a.substring(b,c);c=a.indexOf("category")+9;a='"]'!==a.substring(a.length-2)?a.substring(c,a.length):a.substring(c,a.length-2);sendSearchToGA(b);analyticsBookmarkHistoryClicked=1;changeTab(b,a,1,!0)}}]);var AppTop=angular.module("stSearchHistoryAppTop",["ngSanitize","ngCookies"]);
