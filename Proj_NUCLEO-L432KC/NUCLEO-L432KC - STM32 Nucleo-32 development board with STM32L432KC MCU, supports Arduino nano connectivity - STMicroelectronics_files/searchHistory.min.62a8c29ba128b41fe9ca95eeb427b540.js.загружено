var transform=function(c){return $.param(c)};function compare(c,b){return c.searchDate<b.searchDate?1:c.searchDate>b.searchDate?-1:0}App.filter("getHtmlFilterFromHistory",["$sce",function(c){return function(b){var d=b.indexOf("q\x3d")+2,e=b.indexOf("\x26category\x3d");d=b.substring(d,e);b=b.substring(e+10);"]"===b.substring(b.length-1)&&(b=b.substring(0,b.length-2));"keywords"===b&&(b="all site");return c.trustAsHtml(d.replace(/\\"/g,'"')+"\x3cspan style\x3d'color: #8f8f8f'\x3e\x26nbsp;["+b+"]\x3c/span\x3e")}}]);
