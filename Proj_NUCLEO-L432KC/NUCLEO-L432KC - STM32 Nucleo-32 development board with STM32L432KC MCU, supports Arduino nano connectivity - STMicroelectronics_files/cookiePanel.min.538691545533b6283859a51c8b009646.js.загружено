$(document).ready(function(){var d=function(a,b){$("#"+a+" a.close-cookie-panel").click(function(){c(b);c("st_visited_private_policy");var a=$(this).parent().parent();"cookie-panel-st-cookies-policy"==a.attr("id")?(a.hide(),$("#cookie-panel-st-private-policy").show(),d("cookie-panel-st-private-policy","st_visited_private_policy")):a.hide();return!1})},c=function(a){var b=new Date;b.setMonth(b.getMonth()+13);b="expires\x3d"+b.toGMTString();document.cookie=a+"\x3dtrue; "+b+"; path\x3d/"};0>document.cookie.indexOf("st_visited")?
($("#cookie-panel-st-cookies-policy").show(),d("cookie-panel-st-cookies-policy","st_visited")):0>document.cookie.indexOf("st_visited_private_policy")&&($("#cookie-panel-st-private-policy").show(),c("st_visited_private_policy"),$("#cookie-panel-st-cookies-policy a.close-cookie-panel").click(function(){$(this).parent().parent().hide();return!1}))});