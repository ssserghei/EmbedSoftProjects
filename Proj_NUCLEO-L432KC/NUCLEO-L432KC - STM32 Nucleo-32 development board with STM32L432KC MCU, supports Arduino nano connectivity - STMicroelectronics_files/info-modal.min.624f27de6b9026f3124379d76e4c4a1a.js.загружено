(function(h){function k(){f.off("click").on("click",function(a){a.preventDefault();a={path:"/"};a.expires=new Date(Date.now()+31536E7);a.domain=window.location.host;var b=e;a.expires&&a.expires.toUTCString&&(a.expires=a.expires.toUTCString());b=encodeURIComponent(b)+"\x3d"+encodeURIComponent("1");for(var d in a){b+="; "+d;var g=a[d];!0!==g&&(b+="\x3d"+g)}document.cookie=b;f.off("click");c.fadeOut()})}function d(a){return(a=document.cookie.match("(^|;) ?"+a+"\x3d([^;]*)(;|$)"))?a[2]:null}var c=h(".info-modal"),
